{
  "name": "Meeting Scheduler Workflow",
  "nodes": [
    {
      "id": "1",
      "name": "Chat Trigger",
      "type": "n8n-nodes-langchain.chattrigger",
      "position": [100, 100],
      "parameters": {
        "makePublic": true,
        "mode": "hosted",
        "authentication": "none"
      }
    },
    {
      "id": "2",
      "name": "Parse Input",
      "type": "n8n-nodes-base.function",
      "position": [300, 100],
      "parameters": {
        "functionCode": "const message = items[0].json.message;\nconst parts = message.split(' ');\nif (parts[0] === 'schedule' && parts[1] === 'meeting') {\n    const date = parts[2];\n    const time = parts[3];\n    const attendees = parts.slice(4).map(email => ({ email }));\n    return [ { json: { date, time, attendees } } ];\n} else {\n    return [ { json: { error: 'Invalid format' } } ];\n}"
      }
    },
    {
      "id": "3",
      "name": "Create Event",
      "type": "n8n-nodes-base.googleCalendar",
      "position": [500, 100],
      "parameters": {
        "operation": "createEvent",
        "calendarId": "primary",
        "event": {
          "summary": "Meeting",
          "start": {
            "dateTime": "{{ $json[\"date\"] }}T{{ $json[\"time\"] }}:00"
          },
          "end": {
            "dateTime": "{{ $json[\"date\"] }}T{{ $json[\"time\"] }}:30"
          },
          "attendees": "{{ $json[\"attendees\"] }}"
        },
        "authentication": "googleCalendarApi"
      }
    },
    {
      "id": "4",
      "name": "Respond",
      "type": "n8n-nodes-base.respondtowebhook",
      "position": [700, 100],
      "parameters": {
        "respondWith": "JSON",
        "response": {
          "json": {
            "message": "Meeting scheduled successfully."
          }
        }
      }
    },
    {
      "id": "5",
      "name": "Error Response",
      "type": "n8n-nodes-base.respondtowebhook",
      "position": [500, 250],
      "parameters": {
        "respondWith": "JSON",
        "response": {
          "json": {
            "message": "Error: {{ $json[\"error\"] }}"
          }
        }
      }
    }
  ],
  "connections": {
    "1": {
      "main": [
        {
          "node": "2",
          "type": "main"
        }
      ]
    },
    "2": {
      "main": [
        {
          "node": "3",
          "type": "main"
        },
        {
          "node": "5",
          "type": "main",
          "index": 1
        }
      ]
    },
    "3": {
      "main": [
        {
          "node": "4",
          "type": "main"
        }
      ]
    }
  },
  "active": true,
  "settings": {
    "executionOrder": "v1",
    "saveManualExecutions": true,
    "callerPolicy": "any",
    "errorWorkflow": ""
  },
  "version": 1,
  "meta": {
    "instanceId": "EXAMPLE_INSTANCE_ID"
  },
  "tags": ["meeting", "scheduler"]
}
